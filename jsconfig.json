{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@db/*": ["src/db/*"],
      "@finetune/*": ["src/finetune/*"],
      "@translate/*": ["src/translate/*"],
      "@utils/*": ["src/utils/*"]
    }
  },
  "include": ["src"]
}
